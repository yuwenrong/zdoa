<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.hibernate.auction">
	<class name="wyf.cgq.Groups" table="oa_group">
		<id name="GroupNumber" column="group_number" type="java.lang.String"></id>
		<property name="deptNumber" not-null="true" length="32" column="dept_number"/>
		<property name="groupName" not-null="true" length="50" column="group_name"/>					
	</class>	
</hibernate-mapping>