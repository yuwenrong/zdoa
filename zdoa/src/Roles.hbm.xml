<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.hibernate.auction">
	<class name="wyf.cgq.Roles" table="oa_role">
		<id name="roleNumber" column="role_number" type="java.lang.String"></id>
		<property name="groupNumber" not-null="true" length="32" column="group_number"/>
		<property name="roleName" not-null="true" length="50" column="role_name"/>	
		<property name="roleRight" not-null="true" type="java.lang.Integer" column="role_right"/>				
	</class>	
</hibernate-mapping>